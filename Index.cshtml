@model IEnumerable<Spwin.Models.Student>

@{
    ViewBag.Title = "Index";
}

<h2>Student Details</h2>

<p>
    @Html.ActionLink("Create New", "Create")
   
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StuID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.Password)
        </th>*@
        @*<th>
            @Html.DisplayNameFor(model => model.RePassword)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.Birthday)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RegDay)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Stream)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StuID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>*@
        @*<td>
            @Html.DisplayFor(modelItem => item.RePassword)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.Birthday)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RegDay)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Stream)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new {id=item.StuID}) |
            @Html.ActionLink("Details", "Details", new {id=item.StuID}) |
            @Html.ActionLink("Delete", "Delete", new {id=item.StuID})
        </td>
    </tr>
}

</table>
